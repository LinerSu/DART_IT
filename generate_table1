# Drift on all-ev-pos:
drift.exe -file test/effects/all-ev-pos.ml -prop test/effects/all-ev-pos.yml.prp -ev-trans trans -trace-len 0 -if-part false -out 0 -domain ls -thold true
# evDrift on all-ev-pos:
drift.exe -file test/effects/all-ev-pos.ml -prop test/effects/all-ev-pos.yml.prp -ev-trans direct -trace-len 0 -if-part false -out 0 -domain ls -thold true
